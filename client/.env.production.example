# Production Environment Configuration
# Copy this file to .env.production and update with your production values

# API Configuration (Production URLs)
VITE_API_BASE_URL=https://your-api-domain.com/api
VITE_SOCKET_URL=https://your-api-domain.com

# App Configuration
VITE_APP_NAME=TalentHive
VITE_APP_VERSION=1.0.0

# External Services (Production Keys)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_production_stripe_key
VITE_CLOUDINARY_CLOUD_NAME=your_production_cloudinary_name
VITE_CLOUDINARY_UPLOAD_PRESET=your_production_upload_preset

# Feature Flags (Production Settings)
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SENTRY=true
VITE_ENABLE_DEVTOOLS=false

# Environment
VITE_NODE_ENV=production

# Build Configuration
GENERATE_SOURCEMAP=false
USE_CDN=false
ANALYZE=false

# Security Settings (Production)
VITE_ENABLE_CSP=true
VITE_ENABLE_SECURITY_HEADERS=true